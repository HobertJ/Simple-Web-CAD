{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strictNullChecks": false,
    "sourceMap": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "Main/*": ["./src/*"],
      "Shapes/*": ["./src/2D-Shapes/*"],
      "Base/*": ["./src/Base/*"],
      "Operations/*": ["./src/Operations/*"],
      "Functions/*": ["./src/Functions/*"],
      "Utils/*": ["./src/Utils/*"],
      "Interfaces/*": ["./src/2D-Shapes/Interfaces/*"],
    }
  }
}
