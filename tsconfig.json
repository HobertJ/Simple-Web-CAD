{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "paths": {
      "Main/*": ["./src/*"],
      "2DShapes/*": ["./src/2D-Shapes/*"],
      "Base/*": ["./src/Base/*"],
    }
  }
}
